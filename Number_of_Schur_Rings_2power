S := [1, 1, 3, 10];

Schroder := function(n);
// Given a nonnegative integer n,
// returns the nth Schroder number.
     return &+[Catalan(k)*Binomial(n+k,2*k) : k in [0..n]];
end function;


for n := 5 to 101 do 
S[n] := 2*S[n-1]+4*S[n-2] + 8*S[n-3] //
               - (Catalan(n-2) + Schroder(n-2)) + &+[(Catalan(k-1) //
               + Schroder(k-1) - &+[ Catalan(j) + Schroder(j) //
                : j in [1..k-3]])*S[n-k] : k in [4..n+-1]];
end for;

S;


